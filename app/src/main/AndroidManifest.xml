<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amachay">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.Client.DetailRequestActivity"></activity>
        <activity android:name=".activities.triaje" />
        <activity android:name=".activities.mapa" />
        <activity android:name=".activities.numero_emergencia" />
        <activity android:name=".activities.covid_peru" />
        <activity android:name=".activities.covid_mundial" />
        <activity android:name=".activities.pantalla_sintomas" />
        <activity android:name=".activities.pantalla_novedades" />
        <activity android:name=".activities.pantalla_consejos" />
        <activity android:name=".activities.perfil" />
        <activity android:name=".activities.menu_principal" />
        <activity android:name=".activities.Doctor.MapDoctorActivity" />
        <activity android:name=".activities.Client.MapClientActivity" />
        <activity android:name=".activities.Doctor.RegisterDoctorActivity" />
        <activity
            android:name=".activities.LoginActivity"
            android:parentActivityName=".activities.SelectOptionAuthActivity" />
        <activity
            android:name=".activities.Client.RegisterActivity"
            android:parentActivityName=".activities.SelectOptionAuthActivity" />
        <activity
            android:name=".activities.SelectOptionAuthActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
    </application>

</manifest>